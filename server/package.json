{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "server",
  "title": "Server",
  "description": "",
  "icon": "command-icon.png",
  "author": "",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Update Server",
